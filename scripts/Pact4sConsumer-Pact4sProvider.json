{
  "consumer": {
    "name": "Pact4sConsumer"
  },
  "interactions": [
    {
      "description": "a request to say Goodbye",
      "request": {
        "method": "GET",
        "path": "/goodbye"
      },
      "response": {
        "status": 204
      }
    },
    {
      "description": "a request to say Hello",
      "request": {
        "body": {
          "name": "harry"
        },
        "headers": {
          "Content-Type": "application/json",
          "other-header": "howdy"
        },
        "method": "POST",
        "path": "/hello"
      },
      "response": {
        "body": {
          "hello": "harry"
        },
        "status": 200
      }
    },
    {
      "description": "a request with auth header",
      "request": {
        "generators": {
          "header": {
            "Authorization": {
              "dataType": "STRING",
              "expression": "Bearer ${bearerToken}",
              "type": "ProviderState"
            }
          }
        },
        "headers": {
          "Authorization": "Bearer super-secure"
        },
        "method": "GET",
        "path": "/authorized"
      },
      "response": {
        "status": 200
      }
    },
    {
      "description": "a request to find a friend",
      "providerStates": [
        {
          "name": "bob exists",
          "params": {
            "a": 1,
            "b": [
              "str"
            ],
            "person": "bob"
          }
        }
      ],
      "request": {
        "method": "GET",
        "path": "/anyone-there/bob"
      },
      "response": {
        "body": {
          "found": "bob"
        },
        "status": 200
      }
    },
    {
      "description": "a request to find anyone",
      "providerStates": [
        {
          "name": "nobody is home"
        }
      ],
      "request": {
        "method": "GET",
        "path": "/anyone-there"
      },
      "response": {
        "status": 404
      }
    }
  ],
  "metadata": {
    "pact-jvm": {
      "version": "4.6.9"
    },
    "pactSpecification": {
      "version": "3.0.0"
    }
  },
  "provider": {
    "name": "Pact4sProvider"
  }
}
