{
  "provider": {
    "name": "munit-provider"
  },
  "consumer": {
    "name": "munit-consumer"
  },
  "interactions": [
    {
      "description": "Request to fetch resource with wrong auth",
      "request": {
        "method": "GET",
        "path": "/resource/testID",
        "headers": {
          "Authorization": "Basic dXNlcjp3cm9uZw=="
        }
      },
      "response": {
        "status": 401
      }
    },
    {
      "description": "Request to create new resource",
      "request": {
        "method": "POST",
        "path": "/resource",
        "headers": {
          "Authorization": "Basic dXNlcjpwYXNz"
        },
        "body": {
          "id": "newID",
          "value": 234
        }
      },
      "response": {
        "status": 204
      },
      "providerStates": [
        {
          "name": "resource does not exist"
        }
      ]
    },
    {
      "description": "Request to fetch missing resource",
      "request": {
        "method": "GET",
        "path": "/resource/missingID",
        "headers": {
          "Authorization": "Basic dXNlcjpwYXNz"
        }
      },
      "response": {
        "status": 404
      },
      "providerStates": [
        {
          "name": "resource does not exist"
        }
      ]
    },
    {
      "description": "Request to create resource that already exists",
      "request": {
        "method": "POST",
        "path": "/resource",
        "headers": {
          "Authorization": "Basic dXNlcjpwYXNz"
        },
        "body": {
          "id": "conflict",
          "value": 234
        }
      },
      "response": {
        "status": 409
      },
      "providerStates": [
        {
          "name": "resource exists",
          "params": {
            "id": "conflict",
            "value": 234
          }
        }
      ]
    },
    {
      "description": "Request to fetch extant resource",
      "request": {
        "method": "GET",
        "path": "/resource/testID",
        "headers": {
          "Authorization": "Basic dXNlcjpwYXNz"
        }
      },
      "response": {
        "status": 200,
        "body": {
          "id": "testID",
          "value": 123
        }
      },
      "providerStates": [
        {
          "name": "resource exists",
          "params": {
            "id": "testID",
            "value": 123
          }
        }
      ]
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "3.0.0"
    },
    "pact-jvm": {
      "version": "4.1.39"
    }
  }
}
